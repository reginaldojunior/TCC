{
    "name": "reginaldojunior/TCC",
    "description": "Repository with code and documentation to project of TCC on college Universidade Nove de Julho",
    "keywords": ["TCC", "uninove", "universidade nove de julho"],
    "type": "app",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "respect/rest": "^0.6.0",   
        "respect/relational": "0.5.x",
        "respect/config": "0.3.x",
        "respect/validation": "0.4.x",
        "guzzlehttp/guzzle": "6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PluggTo\\Lib\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}